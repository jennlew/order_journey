(function(){var c=qubit.opentag.CustomTag;var a=new c({"dedupe":false,"locationPlaceHolder":"END","needsConsent":false,"active":true,"url":"","timeout":"5000","async":true,"usesDocumentWrite":false,"name":"[SALES] LivePerson - Order Complete","disabled":false,"id":31010606,"locked":false,"locationObject":"BODY"});var b="productsandservicesbtcom.tags.saleslivepersonordercomplete.Tag";qubit.Define.clientNamespace(b,a);a.htmlContent="\x3cscript type\x3d\"text/javascript\"\x3e\nvar lpTag \x3d lpTag || {};\nlpTag.vars \x3d lpTag.vars  || [];\nlpTag.dbs \x3d lpTag.dbs || [];\n\n// 12 Apr 18 - Mobile hs_comm_cat renamed to hs_device_details\n// 12 Apr 18 - Family Sim added sp_prod_flavour to sku\n// 21 Dec 17 - Further Family SIM updates\n// 09 Nov 17 - Family SIM new and existing count fix (unsuccessful)\n\n// lpTag.autoStart \x3d false; commented out by Ben, Ali and David 17 oct 17\n\n// - QUBIT UV VAR\nvar uv \x3d window.universal_variable;\n\n// ----------------- 1: Sending when the page loads --------------------\n\nwindow.lpTag \x3d window.lpTag || {};\nlpTag.sdes \x3d lpTag.sdes || [];\n\n// Conditions\nvar isPhone \x3d ['PPP', 'PPR', 'PPN', 'PRP', 'PRR', 'PRN', 'PNP', 'PNR', 'PNN', 'PPO', 'POP', 'POO', 'PON', 'PNO'].indexOf(uv.transaction.letter_code) \x3e\x3d 0;\nvar isBB \x3d ['PPP', 'PPR', 'PPN', 'RPP', 'RPR', 'RPN', 'NPP', 'NPR', 'NPN', 'PPO', 'OPP', 'OPO', 'OPN', 'NPO'].indexOf(uv.transaction.letter_code) \x3e\x3d 0;\nvar isTV \x3d ['PPP', 'PRP', 'PNP', 'RPP', 'RRP', 'RNP', 'NPP', 'NRP', 'NNP', 'POP', 'OPP', 'OOP', 'ONP', 'NOP'].indexOf(uv.transaction.letter_code) \x3e\x3d 0;\nvar SIMO \x3d ['PPP', 'PPR', 'PPN', 'RPP', 'RPR', 'RPN', 'NPP', 'NPR', 'NPN', 'PPO', 'OPP', 'OPO', 'OPN', 'NPO'].indexOf(uv.transaction.letter_code_2) \x3e\x3d 0;\nvar Mobile \x3d ['PPP', 'PRP', 'PNP', 'RPP', 'RRP', 'RNP', 'NPP', 'NRP', 'NNP', 'POP', 'OPP', 'OOP', 'ONP', 'NOP'].indexOf(uv.transaction.letter_code_2) \x3e\x3d 0;\nvar FamSim \x3d ['PNN'].indexOf(uv.transaction.letter_code_3) \x3e\x3d 0;\n\nvar lpProducts \x3d [];\n\n// Phone\nif (isPhone) {\n    lpProducts.push(\n        {\n            product: {\n                name: uv.transaction.pstn_mix,\n                category: \"Phone\",\n                sku: uv.transaction.pstn_handset,\n                price: 1\n            },\n            quantity: 1\n        }\n    );\n}\n\n// BB\nif (isBB) {\n    lpProducts.push(\n        {\n            product: {\n                name: uv.transaction.bb_mix,\n                category: \"Broadband\",\n                sku: uv.transaction.bb_order,\n                price: 1\n            },\n            quantity: 1\n        }\n    );\n}\n\n// TV\nif (isTV) {\n    lpProducts.push(\n        {\n            product: {\n                name: uv.transaction.tv_mix,\n                category: \"TV\",\n                sku: uv.transaction.tv_youview,\n                price: 1\n            },\n            quantity: 1\n        }\n    );\n}\n\n// Mobile SIMO\nif (SIMO) {\n    lpProducts.push(\n        {\n            product: {\n                name: uv.transaction.mobile_mix + ' ' + uv.transaction.mobile_price,\n                category: \"Mobile SIMO\",\n                sku: uv.transaction.mobile_mix,\n                price: 1\n            },\n            quantity: 1\n        }\n    );\n}\n\n// Mobile Handset\nif (Mobile) {\n    // 12 Apr 2018\n    /* Qubit tag :  hs_comm_cat is not used for commissioning purpose. This is to be renamed to hs_device_details\n       Existing/New: existing tag Renamed\n       Values: String with HS model name from PMF \u2013 product offering/name\n       PMF attribute: name on the product with crmProductType \u2018Mobile Handset\u2019\n    */\n\n    var deviceDetails \x3d \"\";\n\n    if (typeof(uv.transaction.hs_device_details) !\x3d\x3d 'undefined') {\n        deviceDetails \x3d uv.transaction.hs_device_details;\n    } else {\n        deviceDetails \x3d uv.transaction.hs_comm_cat;\n    }\n\n    lpProducts.push(\n        {\n            product: {\n                name: uv.transaction.hs_prod_flavour + ' ' + uv.transaction.hs_prod_id + ' ' + uv.transaction.hs_contract_duration + ' ' + uv.transaction.hs_monthly_price + ' ' + uv.transaction.hs_plan,\n                sku: deviceDetails,\n                category: 'Mobile Handset',\n                price: 1\n            },\n            quantity: 1\n        }\n    );\n}\n\nif (FamSim) {\n    var existingSims \x3d Number(uv.transaction.splan_exist_sim_count);\n    var newSims \x3d Number(uv.transaction.splan_new_sim_count);\n    var prodFlavour \x3d \"\";\n    var simQuantity \x3d 0;\n\n    if (isNaN(existingSims)) {\n        existingSims \x3d 0;\n    }\n\n    if (isNaN(newSims)) {\n        newSims \x3d 0;\n    }\n\n    // 12 Apr 2018\n    /* sp_prod_flavour added to SKU for reporting\n       number of newSims moved to quantity field\n    */\n    if (typeof(uv.transaction.sp_prod_flavour) !\x3d\x3d 'undefined') {\n        prodFlavour \x3d uv.transaction.sp_prod_flavour;\n        simQuantity \x3d newSims;\n    } else {\n        prodFlavour \x3d newSims;\n        simQuantity \x3d 1;\n    }\n\n    // We only want to push a Family SIM sale if there are new sims added\n    if (newSims \x3e\x3d 1) {\n        if (existingSims \x3e\x3d 1) {\n            // Family SIM - existing (existing sims is \x3e\x3d 1 and new sims is \x3e\x3d 1)\n            lpProducts.push(\n                {\n                    product: {\n                        name: 'Family SIM Existing' + ' ' + uv.transaction.splan_price,\n                        sku: prodFlavour,\n                        category: 'Family SIM Existing',\n                        price: 1\n                    },\n                    quantity: simQuantity\n                }\n            );\n        } else {\n            // Family SIM - new (existing sims is 0 and new sims is \x3e\x3d 1)\n            lpProducts.push(\n                {\n                    product: {\n                        name: 'Family SIM New' + ' ' + uv.transaction.splan_price,\n                        sku: prodFlavour,\n                        category: 'Family SIM New',\n                        price: 1\n                    },\n                    quantity: simQuantity\n                }\n            );\n        }\n    }\n}\n\n// Send products\nif (isPhone || isBB || isTV || SIMO || Mobile || FamSim) {\n    lpTag.sdes.push(\n        {\n            \"type\": \"purchase\", //MANDATORY\n            \"total\": 1, //TOTAL VALUE OF THE TRANSACTION AFTER DISCOUNT\n            \"orderId\": uv.transaction.order_id, //UNIQUE ORDER ID OR RECEIPT ID\n            \"cart\": {\n                \"products\": lpProducts\n            }\n        }\n    );\n}\n\x3c/script\x3e";
